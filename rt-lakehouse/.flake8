[flake8]
max-line-length = 120
ignore = 
    E501,
    W503,
    F403,
    F405,
    E128,
    E124,
    E121,
    E126,
    E127,
    E131,
    E221,
    E225,
    E226,
    E231,
    E241,
    E261,
    E302,
    E305,
    E401,
    E402,
    F401,
    F541,
    F841,
    W291,
    W293,
    E202,
    E702
exclude = 
    .git,
    __pycache__,
    .pytest_cache,
    .venv,
    venv,
    data/,
    .github/
per-file-ignores =
    pipelines/*.py:F403,F405,E402
    tests/test_dedupe.py:F401
    services/time_travel_manager.py:W293
